{
  "name": "dester",
  "version": "1.0.0",
  "description": "A simple CLI js/ts lib-builder that uses Rollup, Babel and TypeScript/Sucrase",
  "keywords": [
    "dester"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wareset/dester",
  "bugs": {
    "url": "https://github.com/wareset/dester/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wareset/dester.git"
  },
  "author": "rease <cftcview@gmail.com>",
  "files": [
    "README.md",
    "bin/**/*",
    "types/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./bin": {
      "import": "./bin/index.mjs",
      "require": "./bin/index.js",
      "types": "./types/index.d.mts"
    }
  },
  "bin": {
    "dester": "dester"
  },
  "scripts": {
    "build": "npm run clear && node ./src/index.mjs --out bin",
    "dev": "nodemon --inspect ./src/index.mjs --watch ./src/index.mjs",
    "clear": "rm -rf -- bin/* && rm -rf -- types/*"
  },
  "peerDependencies": {
    "typescript": "4.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-inject": "^5.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "kleur": "^4.1.5",
    "minimist": "^1.2.7",
    "rollup": "^3.2.5",
    "sucrase": "^3.28.0",
    "terser": "^5.15.1"
  }
}